            <string>draw_self();
draw_set_valign(fa_middle);
switch (action)
{
    case "Run":
    case "Compile+Run":
    case "Feather Compile &amp; Run":
    case "RoomEditor":
    case "Settings":
        draw_set_color(c_black);
        draw_set_halign(fa_center);
        draw_text(x,y,action);
        draw_set_halign(fa_left);
        draw_set_color(c_white);
    break;
    case "DebugMode":
        draw_set_halign(fa_center);
        if (global.runnerDebug)
        {
            draw_set_color(c_black);
            draw_sprite_ext(sprite_index,0,x,y,image_xscale-.2,image_yscale-.4,0,c_black,1);//draw_rectangle(x+8,y+8,x+sprite_width*.5-8,y+sprite_height*.5-8,false);
            draw_set_color(c_white);
            draw_text(x,y,"On");
        }
        else
        {
            draw_set_color(c_black);
            draw_text(x,y,"Off");
            draw_set_color(c_white);
            
        }
        
        draw_text(x,y+sprite_height,"Debug Mode");
        draw_set_halign(fa_left);
        
    break;
    default:
        draw_set_color(c_black);
        draw_set_halign(fa_center);
        draw_text(x,y,action + " Loc");
        draw_set_halign(fa_left);
        var loc = "";
        switch (action)
        {
            case "Cache":
                loc = global.cacheLocation;
            break;
            case "Compiler":
                loc = global.compilerLocation;
            break;
            case "Project":
                loc = global.projectLocation;
            break;
            case "Temp":
                loc = global.tempLocation;
            break;
            case "Runner":
                loc = global.runnerLocation;
            break;
        
        }
        draw_set_color(c_white);
        draw_text(view_xview,y,loc);
    break;

}

draw_set_valign(fa_top);
</string>
