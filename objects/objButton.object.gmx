        <arguments>
          <argument>
            <kind>1</kind>
            <string>
switch (action)
{
    case "Cache":
        var loc = get_string("Insert cache location here:",global.cacheLocation);
        if (loc != "")
        global.cacheLocation = loc;
    break;
    case "Compiler":
        var loc = get_open_filename("*.exe",global.compilerLocation);
        if (loc != "")
        global.compilerLocation = loc;
    break;
    case "Project":
        var loc = get_open_filename("*.project.gmx",global.projectLocation);
        if (loc != "")
        global.projectLocation = loc;
    break;
    case "Temp":
        var loc = get_string("Insert temp file location here:",global.tempLocation);
        if (loc != "")
        global.tempLocation = loc;
        
    break;
    case "Runner":
        var loc = get_open_filename("*.exe",global.runnerLocation);
        if (loc != "")
        global.runnerLocation = loc;
    break;
    case "Run":
        show_debug_message(QuickCompile_Run());
        
    break;
    case "Compile+Run":
    case "Feather Compile &amp; Run":
        if (global.featherweightAutoDelete)
        {
            QuickCompile_CleanTemp(global.tempLocation);
        }
        var doWait = "";
        if (keyboard_check(vk_lshift) || keyboard_check(vk_rshift))
        {
            doWait = "true";
        }
        var doDebug = "false";
        if (global.runnerDebug)
        {
            doDebug = "true";
        }
        var params = global.cacheLocation + "*" + global.tempLocation + "*" + global.compilerLocation + "*" + global.projectLocation + "*" + global.runnerLocation + "*" + doWait + "*" + doDebug;
        if (action == "Feather Compile &amp; Run")
        {
            global.featherweightRooms = get_string("Enter levels you want, spaced apart, without the .room.gmx extension:",global.featherweightRooms);
            params += "*" + global.featherweightRooms_AlwaysKeep
            params += " " + global.featherweightRooms;
            if (global.featherweightExcludeDatafileSearch)
            {
                params += "*-nd";
            }
        }
        show_debug_message(QuickCompile_Compile(params));
        with (objButton)
        {
            if (action == "Run")
            {
                y = ystart;//other.y;
            }
        }
    break;
    case "Settings":
        global.featherweightRooms_AlwaysKeep = get_string("Enter rooms that are strictly necessary for your featherweight project to run, like rooms leading up to your room select: ",global.featherweightRooms_AlwaysKeep);
        global.featherweightExcludeDatafileSearch = get_integer("(Use a 0 for false, 1 for true) Include datafiles in the featherweight search? Use 1 if you don't need external rooms.",global.featherweightExcludeDatafileSearch);
        global.featherweightAutoDelete = get_integer("(Use a 0 for false, 1 for true) Auto-delete previously compiled projects?",global.featherweightAutoDelete);
        //global.featherweightRemoveDatafiles = get_integer("(Use a 0 for false, 1 for true) Remove OGG's from the output? Use 1 if you don't need external rooms.",featherweightExcludeDatafileSearch);
        
    break;
    case "RoomEditor":
        room = lvlEditor;
    break;
    case "Clean Temp":
        get_integer("(Use a 0 for false, 1 for true) Are you sure? This will delete *all* previous compiles made using QuickCompiler.",0);
        QuickCompile_CleanTemp(global.tempLocation);
        show_debug_message("Done!");
    break;
    case "DebugMode":
        global.runnerDebug = !global.runnerDebug;
    break;

}
</string>
          </argument>
        </arguments>
      </action>
    </event>
